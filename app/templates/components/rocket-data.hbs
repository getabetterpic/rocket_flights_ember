<div class="row">
  <form class="col s12">
    <div class="row">
      <div class="input-field col s12">
        {{input placeholder="Name" value=model.name}}
        <label for="">Name</label>
      </div>
      <div class="input-field col s12">
        {{input placeholder="Manufacturer" value=model.manufacturer}}
        <label for="">Manufacturer</label>
      </div>
    </div>
    {{#if model.hasDirtyAttributes}}
      <button type="button" name="button" class="btn" {{action (route-action 'saveRocket' model)}}>Save</button>
    {{else}}
      <button type="button" name="button" class="btn disabled">Save</button>
    {{/if}}
  </form>
</div>
