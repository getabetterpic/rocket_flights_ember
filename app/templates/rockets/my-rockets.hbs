<ul class="collection">
  {{#each model as |userRocket|}}
    {{#link-to 'rockets.show' userRocket.rocket class="collection-item" tagName="li"}}
      <a>{{userRocket.rocketName}}
        <div class="secondary-content">{{userRocket.rocketFlightsLength}}</div>
      </a>
    {{/link-to}}
  {{/each}}
</ul>
<div class="fixed-action-btn" style="bottom: 45px; right: 24px;">
  {{#link-to 'rockets.new' class="btn-floating btn-large red"}}
    <i class="large material-icons">add</i>
  {{/link-to}}
</div>
